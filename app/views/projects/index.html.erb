<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Class type</th>
      <th>Class number</th>
      <th>Class start</th>
      <th>Class finish</th>
      <th>Contact info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= project.Class_Type %></td>
        <td><%= project.Class_Number %></td>
        <td><%= project.Class_Start %></td>
        <td><%= project.Class_Finish %></td>
        <td><%= project.Contact_Info %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
